#include <dt-bindings/gpio/gpio.h>

&tlmm {
	key_factory_reset {
		key_factory_reset_default: key_factory_reset_default {
			pins = "gpio22";
			function = "normal";
			input-enable;
			bias-pull-up;
		};
	};
};

&qupv3_se12_2uart {
		status = "ok";
};

&pcie2 {
		status = "ok";
		qcom,boot-option = <0x0>;
		qcom,core-preset = <0x77777777>;
		pinctrl-0 = <&pcie2_perst_default
				&pcie2_clkreq_default
				&pcie2_wake_default>;
};
